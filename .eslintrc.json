{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "eol-last": "off",
        "max-len": "off",
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "object-curly-spacing": "off",
        "no-unused-vars": "off",
        "new-cap": "off"
    }
}